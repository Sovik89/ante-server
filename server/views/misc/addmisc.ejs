<!DOCTYPE html>
<html lang="en">
  <%- include("./../layouts/head",{pageName: "Add Misc" });%>
  <body>
    <a href="/miscs">All Miscs</a>
    <a href="/dashboard">Dashboard</a>
    <form method="POST" action="/miscs/add-misc" enctype="multipart/form-data">
      <label for="title">Title</label>
      <input type="text" name="title" required />
      <%= possibleErrors.title %>
      <label for="description">Description</label>
      <textarea name="description" rows="14" cols="20" required></textarea>
      <%= possibleErrors.description %>
      <label for="miscImageUrl">miscImageUrl</label>
      <input type="file" name="miscImageUrl" accept="image/*" required />
      <label for="aboutBannerImageUrl">aboutBannerImageUrl</label>
      <input type="file" name="aboutBannerImageUrl" accept="image/*" required />
      <label for="serviceListBannerImageUrl">serviceListBannerImageUrl</label>
      <input
        type="file"
        name="serviceListBannerImageUrl"
        accept="image/*"
        required
      />
      <label for="serviceDetailsBannerImageUrl"
        >serviceDetailsBannerImageUrl</label
      >
      <input
        type="file"
        name="serviceDetailsBannerImageUrl"
        accept="image/*"
        required
      />
      <label for="blogListBannerImageUrl">blogListBannerImageUrl</label>
      <input
        type="file"
        name="blogListBannerImageUrl"
        accept="image/*"
        required
      />
      <label for="contactUsBannerImageUrl">contactUsBannerImageUrl</label>
      <input
        type="file"
        name="contactUsBannerImageUrl"
        accept="image/*"
        required
      />
      <input type="submit" value="SAVE" />
      <input type="reset" value="Reset" />
    </form>
  </body>
</html>
