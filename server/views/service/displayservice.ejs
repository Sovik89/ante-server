<!DOCTYPE html>
<html lang="en">
  <%- include("./../layouts/head",{pageName: "All Service" });%>
  <body>
    <a href="/services/add-service">Add Service</a>
    <%if(services.length==0){%> No Services Present <%} else {%>
    <%services.forEach(service=>{%>
    <div>
      <p><%= service.title %></p>
      <p><%= service.description %></p>
      <a href="/services/edit-service/<%= service.id %>">Edit</a>
      <a href="/services/delete-service/<%= service.id %>">Delete</a>
      <%});}%>
    </div>
  </body>
</html>
