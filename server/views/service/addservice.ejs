<!DOCTYPE html>
<html lang="en">
  <%- include("./../layouts/head",{pageName: "Add Service" });%>
  <body>
    <a href="/services">All Services</a>
      <a href="/dashboard">Dashboard</a>
    <form
      method="POST"
      action="/services/add-service"
      enctype="multipart/form-data"
    >
      <label for="title">Title</label>
      <input type="text" name="title" required />
      <%= possibleErrors.title %>
      <label for="description">Description</label>
      <textarea name="description" rows="14" cols="20" required></textarea>
      <%= possibleErrors.description %>
      <label for="imageUrl">ImageUrl</label>
      <input type="file" name="imageUrl" accept="image/*" required />
      <input type="submit" value="SAVE" />
      <input type="reset" value="Reset" />
    </form>
  </body>
</html>
